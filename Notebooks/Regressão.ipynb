        "plt.title(\"MSE por Modelo\")\n",
        "plt.xticks(rotation=15)\n",
        "\n",
        "# R쑆n",
        "plt.subplot(1, 3, 3)\n",
        "sns.barplot(data=results_df, x=\"Modelo\", y=\"R쑆")\n",
        "plt.title(\"R por Modelo\")\n",
